{
  "name": "koa2-ts-boilerplate",
  "version": "0.1.1",
  "description": "The boilerplate for KOA2, TS and typedi, typeorm.",
  "scripts": {
    "clean": "rm -rf dist/*",
    "start": "PORT=4001 NODE_ENV=development supervisor dist/app.js",
    "copy:config": "cp package.json dist/ && cp yarn.lock dist/ && cp process.yml dist/",
    "build": "yarn clean && tsc && yarn copy:config",
    "dev": "yarn clean && tsc -w",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "mocha test/*.test.js --compilers js:babel-register --require test/_helpers",
    "test-watch": "npm run test -- --watch"
  },
  "author": {
    "name": "Lanvige Jiang"
  },
  "contributors": [
    {
      "name": "Lanvige Jiang"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.3",
    "@types/bluebird-global": "^3.5.2",
    "@types/cli-color": "^0.3.29",
    "@types/fs-extra-promise": "^1.0.1",
    "@types/koa": "^2.0.43",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.27",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.0",
    "@types/node-zookeeper-client": "^0.2.6",
    "@types/request-promise": "^4.1.40",
    "@types/should": "^13.0.0",
    "class-validator": "^0.8.5",
    "glob": "^7.1.2",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.3.0",
    "md5": "^2.2.1",
    "mysql2": "^1.5.3",
    "node-rules": "^3.1.0",
    "node-zookeeper-client": "^0.2.2",
    "os": "^0.1.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "routing-controllers": "^0.7.6",
    "tslib": "^1.9.0",
    "typedi": "^0.7.1",
    "typeorm": "^0.1.11",
    "typeorm-typedi-extensions": "^0.1.1",
    "underscore": "^1.8.3",
    "versioning": "^1.1.0"
  },
  "devDependencies": {
    "shipit-deploy": "^4.0.2",
    "shipit-shared": "^4.4.2",
    "shipit-shared-copy": "^0.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2"
  }
}
